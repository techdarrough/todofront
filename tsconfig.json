{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ES6",
    "module": "esnext",
    "outDir": "./Build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
, "build/index.js", "build/components/Def.js", "build/components/Header.js", "build/Pages/Create.js", "build/Pages/Edit.js", "build/Pages/Main.js", "build/App.js", "build/components/LowerMenu.js"  ]
}
